    <DataSet Name="unsavedAttendance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Term">
            <Value>=Parameters!Term.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@calendariD">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT staff.staffFullName as 'Teacher (Att not verified)'
,  convert(varchar(10),d.date,111) + ' ' + left(datename(dw,d.date),3) as 'Missing Att on'
, p.name as 'Per'
, crs.courseName
, Cast(crs.courseNumber as varchar(4)) + '-' + cast(crs.sectionNumber as varchar(4)) as 'Crs-Scn'
--, crs.studentCount as '#Stus in scn today'
, crs.roomName as 'Room'
, ISNULL(contact.workphone,'') as 'Work phone'
, crs.[Teacher Email]
, term.name as 'Term'
, convert(varchar(6),term.startDate,107) + ' - '    /* Note dates checked - but didn't look past today */
  + CASE WHEN Convert(varchar(10),term.endDate,112) &lt;= Convert(varchar(10),GETDATE(),112) THEN convert(varchar(12),term.endDate,107) ELSE convert(varchar(12),GETDATE(),107) END
  as 'Att dates checked'
, cal.name as Calendar
, CASE WHEN crs.sectionModifiedDate &gt;= d.date  /* if edits made after missing attendance date, then report who */
       THEN convert(varchar(10),crs.sectionModifiedDate,110) + ' by ' + i.firstname + ' ' + i.lastname 
	   ELSE ''
END  AS 'Sched mod after att date'
FROM v_CourseSection crs 
INNER JOIN Trial t on t.trialID=crs.trialID and t.active=1   /* active trial */
INNER JOIN SectionPlacement sp on sp.sectionID=crs.sectionID and sp.trialID=t.trialID
INNER JOIN Term on term.termID=sp.termID and term.termID = @Term
INNER JOIN Period p on p.periodID=sp.periodID
INNER JOIN Calendar cal on cal.calendarID=crs.calendarID and cal.startDate &lt;= term.startDate and cal.endDate &gt;= term.endDate  /* Attendance dates fall within this calendar */
INNER JOIN [Day] d on d.calendarID=crs.calendarID and d.periodScheduleID=p.periodScheduleID and d.attendance=1   /* Attendance day in the term; don't use past today */
           and d.date &gt;= term.startDate and d.date &lt;= CASE WHEN Convert(varchar(10),term.endDate,112) &lt;= Convert(varchar(10),GETDATE(),112) THEN term.endDate ELSE cast(GETDATE() as smalldatetime) END
INNER JOIN v_StaffHistory staff on staff.personID=crs.personID and staff.sectionID=crs.sectionID and (staff.assignmentStartDate is null or staff.assignmentStartDate &lt;= term.startDate) and (staff.assignmentEndDate is null or staff.assignmentEndDate &gt;= term.endDate)
LEFT JOIN Contact on contact.personID=staff.personID   /* Teacher's contact info */
LEFT JOIN Individual i on i.personID=crs.sectionModifiedByID
LEFT JOIN AttendanceCounts ac on ac.sectionID=crs.sectionID and ac.date = d.date  /* look for Counts saved on an attendance day */
                and (ac.periodsBitmask &amp; power(2,(p.seq-1)) = power(2,(p.seq-1)))  /* bitmask is in order of period sequence, so Bit&amp; operation tells if this period exists */
where 1=1
and crs.active = 1      /* course is active */
and crs.attendance=1    /* course takes attendance */
and crs.studentCount&gt;0  /* course section has students today - need to remove this if running after term has ended */
and cal.calendarID in (@calendariD)
and ac.sectionID is null   /* no attendance saved */
-----------------------------------------------------------------------------------------
-- Check to be sure at least one student is on the roster in the timeframe being checked
-----------------------------------------------------------------------------------------
and EXISTS (SELECT 1 FROM Roster WHERE roster.sectionID=crs.sectionID and roster.trialID=t.trialID and (roster.startdate is null or roster.startdate &lt;= term.startDate) and (roster.enddate is null or roster.enddate &gt;= term.endDate) )

/* add in if doing all schools */ --and right(cal.name,20) &lt;&gt; 'Community Day School' and left(cal.name,5) &lt;&gt; 'EDOP '  /* Amanda said too many retro-scheduling, so she said to exclude CDS and EDOP */</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
