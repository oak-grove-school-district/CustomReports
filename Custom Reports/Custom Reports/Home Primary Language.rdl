    <DataSource Name="DataSource1">
      <DataSourceReference>campus</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>cc384c4a-e2de-4d32-a42b-a9b73c791596</rd:DataSourceID>
