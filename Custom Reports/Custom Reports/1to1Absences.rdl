  <DataSets>
    <DataSet Name="absences">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select cal.name, p.studentNumber, i.lastName, i.firstName, coalesce(att.status,ae.code) as [Att Code], att.date
from Person p
	join [Identity] i on i.identityID = p.currentIdentityID
	join Enrollment e on e.personID = p.personID
	join Calendar cal on cal.calendarID = e.calendarID
		and cal.summerSchool = 0
	join Attendance att on att.personID = p.personID
		and att.calendarID = cal.calendarID
	left join AttendanceExcuse ae on ae.excuseID = att.excuseID
	join cust_og_programMatrix cpm on cpm.personID = att.personID
		and cpm.[1:1] = 'Y'
where att.date = convert(varchar(10), getdate(), 120)
	and (att.status = 'A' or ae.status = 'A')
group by cal.name, p.studentNumber, i.lastName, i.firstName, att.status,  ae.code, att.date</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="studentNumber">
          <DataField>studentNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lastName">
