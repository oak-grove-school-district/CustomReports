  <DataSets>
    <DataSet Name="campus">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select cal.name as [School]
	, cs.name as [Course]
	, i.lastName
	, i.firstName
	, con.email
                  , p.personID
from Calendar cal
	join Trial tr on tr.calendarID = cal.calendarID
		and tr.active = 1
	join Course cs on cs.calendarID = cal.calendarID
	join Section sec on sec.courseID = cs.courseID
		and sec.trialID = tr.trialID
	join SectionStaffHistory ssh on ssh.sectionID = sec.sectionID
		and (ssh.startDate is null or ssh.startDate &lt;= getdate())
		and (ssh.endDate is null or ssh.endDate &gt;= getdate())
	join Person p on p.personID = ssh.personID
	join [Identity] i on i.personID = p.personID
		and i.identityID = p.currentIdentityID
	join Contact con on con.personID = p.personID
	join EmploymentAssignment ea on ea.personID = p.personID
		and (ea.title &lt;&gt; 'Long-Term Sub' and ea.title &lt;&gt; 'Student Teacher')
		and ea.startDate &lt;= getdate()
		and (ea.endDate is null or ea.endDate &gt;= getdate())
where cal.endYear = 2024
group by cal.name
	, cs.name
	, i.lastName
	, i.firstName
	, con.email
                  , p.personID</CommandText>
      </Query>
