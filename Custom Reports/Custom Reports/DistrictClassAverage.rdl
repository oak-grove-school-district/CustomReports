  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@asOfDate">
            <Value>=Parameters!asOfDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select avg(x.classSize) as [Average Class Size], x.school
from (
	select cast(count(p.studentNumber) as decimal(5,2)) as classSize, cs.name, sec.sectionID, cal.name as school
	from Person p
		join Enrollment e on e.personID = p.personID
			and e.active = 1
			and e.startDate &lt;= @asOfDate
			and (e.endDate is null or e.endDate &gt;= @asOfDate)
		join Calendar cal on cal.calendarID = e.calendarID
			and cal.endYear = e.endYear
		join SchoolYear sy on sy.endYear = cal.endYear
			--and sy.active = 1
		join Course cs on cs.calendarID = cal.calendarID
		join Section sec on sec.courseID = cs.courseID
		join Roster ros on ros.sectionID = sec.sectionID
			and ros.personID = p.personID
			and (ros.startDate is null or ros.startDate &lt;= @asOfDate)
			and (ros.endDate is null or ros.endDate &gt;= @asOfDate)
	where (cal.name not like '%bernal%' and cal.name not like '%davis%' and cal.name not like '%herman%' and cal.name not like '%academy%' and cal.name not like '%hospital%')
		and(cs.name like 'TK Homeroom' or cs.name like 'TK/00 Homeroom' or cs.name like '00 Homeroom' or cs.name like '00/01 Homeroom' or cs.name like '01 Homeroom' or cs.name like '01/02 Homeroom' or cs.name like '02 Homeroom' or cs.name like '02/03 Homeroom' or cs.name like '03 Homeroom' or cs.name like '03/04 Homeroom')
	group by cs.name, sec.sectionID, cal.name
	--order by cal.name
	) x
group by x.school</CommandText>
      </Query>
      <Fields>
        <Field Name="Average_Class_Size">
          <DataField>Average Class Size</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="school">
          <DataField>school</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
