  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>campus</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e1f5d785-a646-4670-b1ae-2fcaa5b10322</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@asOfDate">
            <Value>=Parameters!asOfDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        AVG(classSize) AS [Average Class Size], school
FROM            (SELECT        COUNT(p.studentNumber) AS classSize, cs.name, sec.sectionID, cal.name AS school
                          FROM            Person AS p INNER JOIN
                                                    Enrollment AS e ON e.personID = p.personID AND e.active = 1 AND e.startDate &lt;= @asOfDate AND (e.endDate IS NULL OR
                                                    e.endDate &gt;= @asOfDate) INNER JOIN
                                                    Calendar AS cal ON cal.calendarID = e.calendarID AND cal.endYear = e.endYear INNER JOIN
                                                    SchoolYear AS sy ON sy.endYear = cal.endYear AND sy.active = 1 INNER JOIN
                                                    Course AS cs ON cs.calendarID = cal.calendarID INNER JOIN
                                                    Section AS sec ON sec.courseID = cs.courseID INNER JOIN
                                                    Roster AS ros ON ros.sectionID = sec.sectionID AND ros.personID = p.personID AND (ros.startDate IS NULL OR
                                                    ros.startDate &lt;= @asOfDate) AND (ros.endDate IS NULL OR
                                                    ros.endDate &gt;= @asOfDate)
                          WHERE        (cal.name NOT LIKE '%bernal%') AND (cal.name NOT LIKE '%davis%') AND (cal.name NOT LIKE '%herman%') AND (cal.name NOT LIKE '%academy%') AND (cal.name NOT LIKE '%hospital%') AND 
                                                    (cs.name LIKE '%TK%' OR
                                                    cs.name LIKE '%TK/00%' OR
                                                    cs.name LIKE '%00%' OR
                                                    cs.name LIKE '%00/01%' OR
                                                    cs.name LIKE '%01%' OR
                                                    cs.name LIKE '%01/02%' OR
                                                    cs.name LIKE '%02%' OR
                                                    cs.name LIKE '%02/03%' OR
                                                    cs.name LIKE '%03%' OR
                                                    cs.name LIKE '%03/04%')
                          GROUP BY cs.name, sec.sectionID, cal.name) AS x
GROUP BY school</CommandText>
      </Query>
      <Fields>
        <Field Name="Average_Class_Size">
          <DataField>Average Class Size</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="school">
          <DataField>school</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
