<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>campus</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>738ad1fd-b669-43f8-9371-2e57bc14f915</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="userCalendar"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@userID"><Value>=Parameters!userID.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @allCalendars bit
SELECT @allCalendars = u.allCalendars FROM dbo.UserAccount u WHERE u.userID = @userID

IF @allCalendars = 1
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as Calendar, 1 as ModifyRights, 1 as ownerRights
	FROM dbo.Calendar c
	INNER JOIN school s on s.schoolID = c.schoolID
	order by calendarID desc

END

ELSE
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as calendar , mr.modifyRights, mr.ownerRights
	FROM dbo.UserAccount ua
	LEFT OUTER JOIN dbo.UserSchoolYearRights mr on mr.userID = ua.userID
	INNER JOIN dbo.Calendar c on (mr.districtID = c.districtID)
					AND ((c.endYear = mr.endYear AND c.schoolID = mr.schoolID AND mr.calendarID IS NULL)
					OR (c.endYear = mr.endYear AND mr.schoolID IS NULL)
					OR (c.schoolID = mr.schoolID AND mr.endYear IS NULL)
					OR (c.calendarID = mr.calendarID)
					OR (mr.endYear IS NULL AND mr.schoolID IS NULL))
	INNER JOIN school s on s.schoolID = c.schoolID
	WHERE ua.userID = @userID
	UNION
	SELECT c2.districtID, c2.schoolID, c2.calendarID, c2.name + ' - ' + s2.name as calendar, mr2.modifyRights, mr2.ownerRights
	FROM dbo.UserGroupSchoolYearRights mr2
	INNER JOIN dbo.Calendar c2 on mr2.districtID = c2.districtID
					AND ((c2.endYear = mr2.endYear AND c2.schoolID = mr2.schoolID AND mr2.calendarID IS NULL)
					OR (c2.endYear = mr2.endYear AND mr2.schoolID IS NULL)
					OR (c2.schoolID = mr2.schoolID AND mr2.endYear IS NULL)
					OR (c2.calendarID = mr2.calendarID)
					OR (mr2.endYear IS NULL AND mr2.schoolID IS NULL))
	INNER JOIN UserGroupMember gm2 on gm2.groupID = mr2.groupID
	INNER JOIN school s2 on s2.schoolID = c2.schoolID
	WHERE gm2.userID = @userID
	ORDER BY calendarID DESC

END</CommandText></Query><Fields><Field Name="districtID"><DataField>districtID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="schoolID"><DataField>schoolID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="calendarID"><DataField>calendarID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Calendar"><DataField>Calendar</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ModifyRights"><DataField>ModifyRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ownerRights"><DataField>ownerRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="DataSet1"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@month"><Value>=Parameters!month.Value</Value></QueryParameter><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        teacherDisplay, tEnrolled, tAbs, [% Attendance]
FROM            (SELECT        teacherDisplay, SUM(tEnrolled) AS tEnrolled, SUM(tAbs) AS tAbs, CAST(100 - 100 * SUM(tAbs) / SUM(tEnrolled) AS decimal(6, 2)) AS [% Attendance]
                          FROM            (SELECT        p.studentNumber, e.grade, i.lastName, i.firstName, sec.teacherDisplay, MAX(dbo.cust_fn_TotalInstDays(CASE WHEN (e.endDate &gt;= tpos.mEndDate OR
                                                                              e.endDate IS NULL) THEN tpos.mEndDate ELSE e.endDate END, CASE WHEN e.startDate &lt;= tpos.mStartDate THEN tpos.mStartDate ELSE e.startDate END)) AS tEnrolled, CASE WHEN tabs.tAbs IS NULL 
                                                                              THEN 0 ELSE tabs.tAbs END AS tAbs
                                                    FROM            Person AS p INNER JOIN
                                                                              [Identity] AS i ON i.personID = p.personID AND i.identityID = p.currentIdentityID INNER JOIN
                                                                              Enrollment AS e ON e.personID = p.personID AND e.active = 1 AND e.startDate &lt; CAST(@month + 1 AS varchar) + '/01/' + CAST(YEAR(GETDATE()) AS varchar) AND (e.endDate IS NULL OR
                                                                              e.endDate &gt; CAST(@month + 1 AS varchar) + '/01/' + CAST(YEAR(GETDATE()) AS varchar)) INNER JOIN
                                                                              Calendar AS cal ON cal.calendarID = e.calendarID AND cal.endYear = e.endYear AND cal.calendarID = @calendarID INNER JOIN
                                                                              SchoolYear AS sy ON sy.endYear = cal.endYear AND sy.active = 1 INNER JOIN
                                                                              ScheduleStructure AS ss ON ss.calendarID = cal.calendarID INNER JOIN
                                                                              Trial AS tr ON tr.structureID = ss.structureID AND tr.calendarID = cal.calendarID AND tr.active = 1 INNER JOIN
                                                                              Course AS cs ON cs.calendarID = cal.calendarID INNER JOIN
                                                                              Section AS sec ON sec.courseID = cs.courseID AND sec.trialID = tr.trialID INNER JOIN
                                                                              Roster AS ros ON ros.personID = p.personID AND ros.sectionID = sec.sectionID AND ros.trialID = tr.trialID AND (ros.endDate IS NULL OR
                                                                              MONTH(ros.endDate) &gt;= @month) INNER JOIN
                                                                              PeriodSchedule AS ps ON ps.structureID = ss.structureID AND ps.structureID = tr.structureID INNER JOIN
                                                                              Period AS per ON per.periodScheduleID = ps.periodScheduleID LEFT OUTER JOIN
                                                                                  (SELECT        MIN(Date) AS mStartDate, MAX(Date) AS mEndDate, calendarID
                                                                                    FROM            (SELECT        CASE WHEN month([Day].date) = @month THEN [Day].date END AS Date, calendarID
                                                                                                              FROM            Day
                                                                                                              WHERE        (calendarID = @calendarID) AND (attendance = 1)) AS x_1
                                                                                    GROUP BY calendarID) AS tPos ON tPos.calendarID = cal.calendarID LEFT OUTER JOIN
                                                                                  (SELECT        att.personID, att.calendarID, att.periodID, COUNT(att.date) AS tAbs
                                                                                    FROM            Attendance AS att INNER JOIN
                                                                                                              AttendanceExcuse AS ae ON ae.excuseID = att.excuseID AND att.calendarID = ae.calendarID AND ae.calendarID = @calendarID
                                                                                    WHERE        (MONTH(att.date) = @month) AND (ae.status = 'A')
                                                                                    GROUP BY att.personID, att.calendarID, att.periodID) AS tAbs ON tAbs.personID = p.personID AND tAbs.calendarID = cal.calendarID
                                                    GROUP BY p.studentNumber, e.grade, i.lastName, i.firstName, sec.teacherDisplay, tAbs.tAbs) AS z
                          GROUP BY teacherDisplay) AS x
ORDER BY [% Attendance] DESC</CommandText></Query><Fields><Field Name="teacherDisplay"><DataField>teacherDisplay</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="tEnrolled"><DataField>tEnrolled</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="tAbs"><DataField>tAbs</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ID__Attendance"><DataField>% Attendance</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>2.10417in</Width></TablixColumn><TablixColumn><Width>1in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.25in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox1"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Teacher</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox1</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox3"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>% Attendance</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox3</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell></TablixCells></TablixRow><TablixRow><Height>0.25in</Height><TablixCells><TablixCell><CellContents><Textbox Name="teacherDisplay"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!teacherDisplay.Value</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>teacherDisplay</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="ID__Attendance"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!ID__Attendance.Value</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>ID__Attendance</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell></TablixCells></TablixRow></TablixRows></TablixBody><TablixColumnHierarchy><TablixMembers><TablixMember /><TablixMember /></TablixMembers></TablixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="Details" /></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>DataSet1</DataSetName><Height>0.5in</Height><Width>3.10417in</Width><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>0.5in</Height><Style /></Body><Width>3.10417in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page></ReportSection></ReportSections><ReportParameters><ReportParameter Name="nodeID"><DataType>String</DataType><DefaultValue><Values><Value>1</Value></V