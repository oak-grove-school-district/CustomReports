<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="campus"><DataSourceReference>campus</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>8bd7e783-d508-4229-84ce-ca7f5aa60d37</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="AllStudents"><Query><DataSourceName>campus</DataSourceName><QueryParameters><QueryParameter Name="@startDate"><Value>=Parameters!startDate.Value</Value></QueryParameter><QueryParameter Name="@choiceDate"><Value>=Parameters!choiceDate.Value</Value></QueryParameter><QueryParameter Name="@attendanceCodes"><Value>=split(Parameters!attendanceCodes.Value,",")</Value></QueryParameter><QueryParameter Name="@endYear"><Value>=Parameters!endYear.Value</Value></QueryParameter><QueryParameter Name="@threshold"><Value>=Parameters!threshold.Value</Value></QueryParameter><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter></QueryParameters><CommandText>with Ab (personID, calendarName, studentNumber, lastName, firstName, grade, CorrLang, [Percent], [Incident #], [date], [Code/Contact Mode], comments) as (
select y.personID, y.calendarName, y.studentNumber, y.lastName, y.firstName, y.grade, y.CorrLang
, cast(y.maxIn as float) / (cast(dbo.cust_fn_InstDayNumber(@choiceDate) as float) - (cast(dbo.cust_fn_InstDayNumber(y.minDate) as float) - 1)) as [Percent]
, y.[Incident #], y.date, y.[Code/Contact Mode], y.comments
from (
select x.personID, x.calendarName, x.studentNumber, x.lastName, x.firstName, x.grade, x.CorrLang
, ROW_NUMBER() over(PARTITION by x.[studentNumber] order by x.[date] asc) as [Incident #]
, x.date, x.[Code/Contact Mode], x.comments
, COUNT(x.[date]) OVER (PARTITION BY x.studentNumber) as maxIn, x.minDate
from (
select c.calendarID, c.name as [calendarName], r.personID, p.studentNumber, i.lastName, i.firstName, e.grade, cd.name as [CorrLang]
, min(e.startDate) as minDate, np.date, np.code as [Code/Contact Mode], np.comments
from Roster r
join Person p on p.personID = r.personID
join [Identity] i on i.personID = p.personID and i.identityID = p.currentIdentityID
left join CustomStudent cs on cs.personID = p.personID and cs.attributeID = 341
left join CampusDictionary cd on cd.attributeID = cs.attributeID and cd.code = cs.value
join Section se on se.sectionID = r.sectionID
join Course co on co.courseID = se.courseID and co.active = 1 and co.attendance = 1
join Calendar c on c.calendarID = co.calendarID and c.endYear = @endYear
join Enrollment e on e.personID = p.personID and e.calendarID = c.calendarID and e.grade not in ('PK','TK')
join Trial tr on tr.calendarID = c.calendarID and tr.trialID = r.trialID and tr.active = 1
join SectionPlacement sp on sp.sectionID = r.sectionID and sp.trialID = r.trialID
join PeriodSchedule ps on ps.structureID = tr.structureID
join Period pe on pe.periodScheduleID = ps.periodScheduleID and pe.periodID = sp.periodID and pe.nonInstructional = 0
join Day on day.calendarID = co.calendarID and day.periodScheduleID = ps.periodScheduleID
join Term on term.termID = sp.termID and day.date between term.startDate and term.endDate
join (
select at.personID, at.date, ae.code, at.comments, count(at.periodID) as [pAbsent]
from Attendance at
join AttendanceExcuse ae on ae.excuseID = at.excuseID and ae.calendarID = at.calendarID and ae.code in (@attendanceCodes)
where at.calendarID = @calendarID and at.date between @startDate and @choiceDate
group by at.personID, at.date, ae.code, at.comments
) np on np.personID = r.personID and np.date = day.date
where (r.startDate &lt;= np.date or r.startDate is null) and (r.endDate &gt;= np.date or r.endDate is null)
group by c.calendarID, c.name, r.personID, p.studentNumber, i.lastName, i.firstName, e.grade, cd.name, np.date, np.code, np.pAbsent, np.comments
having (np.code in (@attendanceCodes) and np.pAbsent = count(sp.periodID)) or (np.code = 'TD30')
) x
left join enrollment ec on ec.calendarID = x.calendarID and ec.endDate is null
where ec.calendarID = @calendarID
group by x.calendarName, x.personID, x.studentNumber, x.lastName, x.firstName, x.grade, x.CorrLang
, x.date, x.[Code/Contact Mode], x.comments, x.minDate
) y
where y.maxIn &gt;= @threshold
)

select *
from ab
union
select ab.personID, ab.calendarName, ab.studentNumber, ab.lastName, ab.firstName, ab.grade, ab.CorrLang, ab.[Percent]
, null
, cl.dateTimeStamp as [Date], cl.contactMode as [Code/Contact Mode], cl.[text] as [Comments]
from ab
join ContactLog cl on cl.personID = ab.personID and cl.module = 'PLP' and (convert(date,cl.dateTimeStamp) between @startDate and @choiceDate)
order by ab.lastName, ab.firstName, ab.date</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="personID"><DataField>personID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="calendarName"><DataField>calendarName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="studentNumber"><DataField>studentNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="lastName"><DataField>lastName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="firstName"><DataField>firstName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="grade"><DataField>grade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Incident__"><DataField>Incident #</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="CorrLang"><DataField>CorrLang</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Percent"><DataField>Percent</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="Code_Contact_Mode"><DataField>Code/Contact Mode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Date"><DataField>date</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="Comments"><DataField>comments</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="userCalendar"><Query><DataSourceName>campus</DataSourceName><QueryParameters><QueryParameter Name="@userID"><Value>=Parameters!userID.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @allCalendars bit
SELECT @allCalendars = u.allCalendars FROM dbo.UserAccount u WHERE u.userID = @userID

IF @allCalendars = 1
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as Calendar, 1 as ModifyRights, 1 as ownerRights
	FROM dbo.Calendar c
	INNER JOIN school s on s.schoolID = c.schoolID
	order by calendarID desc

END

ELSE
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as calendar , mr.modifyRights, mr.ownerRights
	FROM dbo.UserAccount ua
	LEFT OUTER JOIN dbo.UserSchoolYearRights mr on mr.userID = ua.userID
	INNER JOIN dbo.Calendar c on (mr.districtID = c.districtID)
					AND ((c.endYear = mr.endYear AND c.schoolID = mr.schoolID AND mr.calendarID IS NULL)
					OR (c.endYear = mr.endYear AND mr.schoolID IS NULL)
					OR (c.schoolID = mr.schoolID AND mr.endYear IS NULL)
					OR (c.calendarID = mr.calendarID)
					OR (mr.endYear IS NULL AND mr.schoolID IS NULL))
	INNER JOIN school s on s.schoolID = c.schoolID
	WHERE ua.userID = @userID
	UNION
	SELECT c2.districtID, c2.schoolID, c2.calendarID, c2.name + ' - ' + s2.name as calendar, mr2.modifyRights, mr2.ownerRights
	FROM dbo.UserGroupSchoolYearRights mr2
	INNER JOIN dbo.Calendar c2 on mr2.districtID = c2.districtID
					AND ((c2.endYear = mr2.endYear AND c2.schoolID = mr2.schoolID AND mr2.calendarID IS NULL)
					OR (c2.endYear = mr2.endYear AND mr2.schoolID IS NULL)
					OR (c2.schoolID = mr2.schoolID AND mr2.endYear IS NULL)
					OR (c2.calendarID = mr2.calendarID)
					OR (mr2.endYear IS NULL AND mr2.schoolID IS NULL))
	INNER JOIN UserGroupMember gm2 on gm2.groupID = mr2.groupID
	INNER JOIN school s2 on s2.schoolID = c2.schoolID
	WHERE gm2.userID = @userID
	ORDER BY calendarID DESC

END</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="districtID"><DataField>districtID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="schoolID"><DataField>schoolID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="calendarID"><DataField>calendarID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Calendar"><DataField>Calendar</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ModifyRights"><DataField>ModifyRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ownerRights"><DataField>ownerRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="grade"><Query><DataSourceName>campus</DataSourceName><QueryParameters><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT distinct e.grade, gl.seq
FROM enrollment e
INNER JOIN gradelevel gl on gl.[name] = e.grade AND gl.calendarID = e.calendarID and e.grade not in ('PK','SO')
WHERE e.calendarID in (@calendarID)
ORDER BY gl.seq</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="grade"><DataField>grade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="seq"><DataField>seq</DataField><rd:TypeName>System.Byte</rd:TypeName></Field></Fields></DataSet><DataSet Name="settings"><Query><DataSourceName>campus</DataSourceName><QueryParameters><QueryParameter Name="@letterName"><Value>=Parameters!letterName.Value</Value></QueryParameter></QueryParameters><CommandText>select case
when @letterName like 'Excessive Illness%' then 'ILL' 
else 'OTX,UNV,TD30,CUT' end as [code]</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="code"><DataField>code</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="thresholds"><Query><DataSourceName>campus</DataSourceName><QueryParameters><QueryParameter Name="@letterName"><Value>=Parameters!letterName.Value</Value></QueryParameter><QueryParameter Name="@choiceDate"><Value>=Parameters!choiceDate.Value</Value></QueryParameter><QueryParameter Name="@startDate"><Value>=Parameters!startDate.Value</Value></QueryParameter></QueryParameters><CommandText>select
case 
when @letterName like 'Form A%' then 3.0
when @letterName like 'Form B%' then 5.0
when @letterName like 'Soft Letter%' then 2.0
when @letterName like 'Excessive Illness 1%' then (cast(x.InstDay as float) - cast(x.StartDay as float)) / 10
when @letterName like 'Excessive Illness 2%' then ((cast(x.InstDay as float) - cast(x.StartDay as float)) / 10) + 1
end as [threshold]
from
(select dbo.cust_fn_InstDayNumber(@choiceDate) as InstDay
, (select dbo.cust_fn_InstDayNumber(@startDate) - 1) as StartDay
) x</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="threshold"><DataField>threshold</DataField><rd:TypeName>System.Double</rd:TypeName></Field></Fields></DataSet><DataSet Name="EndYear"><Query><DataSourceName>campus</DataSourceName><CommandText>SELECT        endYear
FROM            SchoolYear AS sy
WHERE        (active = 1)</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="endYear"><DataField>endYear</DataField><rd:TypeName>System.Int16</rd:TypeName></Field></Fields></DataSet><DataSet Name="StartDate"><Query><DataSourceName>campus</DataSourceName><QueryParameters><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        c.startDate
FROM            Calendar AS c INNER JOIN
                         SchoolYear AS sy ON sy.endYear = c.endYear AND sy.active = 1
where c.calendarID in (@calendarID)</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="startDate"><DataField>startDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>0.5625in</Width></TablixColumn><TablixColumn><Width>1.34375in</Width></TablixColumn><TablixColumn><Width>1.20833in</Width></TablixColumn><TablixColumn><Width>3.23959in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.14583in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox10"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value /><Style><FontSize>8pt</FontSize></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox10</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><ColSpan>4</ColSpan></CellContents></TablixCell><TablixCell /><TablixCell /><TablixCell /></TablixCells></TablixRow><TablixRow><Height>0.14583in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox72"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Incident #</Value><Style><FontSize>8pt</FontSize></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox72</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox74"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRun