<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>campus</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>ce63d925-88df-4da9-a83d-bbedc2b1edf5</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="GPA"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter></QueryParameters><CommandText>select x.studentNumber, x.lastName, x.firstName, x.grade
	, max(case when x.grade = '07' then x.[7th Sem 1]
		   when x.grade = '08' then x.[8th Sem 1] end) as [Semster 1]
	, max(case when x.grade = '07' then x.[7th Sem 2]
		when x.grade = '08' then x.[8th Sem 2] end) as [Semster 2]
	, x.addressLine1, x.addressLine2
from(
select p.studentNumber, i.lastName, i.firstName, e.grade
	, max(case when cust.attributeID = 546 then cust.value end) as [7th Sem 1]
	, max(case when cust.attributeID = 547 then cust.value end) as [7th Sem 2]
	, max(case when cust.attributeID = 548 then cust.value end) as [8th Sem 1]
	, max(case when cust.attributeID = 549 then cust.value end) as [8th Sem 2]
	, ccsl.addressLine1, ccsl.addressLine2
FROM Person p 
	JOIN [Identity] i ON i.personID = p.personID 
		AND i.identityID = p.currentIdentityID 
	JOIN Enrollment e ON e.personID = p.personID 
		AND e.active = 1
		and (e.endDate is null or e.endDate &gt;= getdate())
	JOIN Calendar cal ON cal.calendarID = e.calendarID 
		AND cal.endYear = e.endYear 
	JOIN SchoolYear sy ON sy.endYear = cal.endYear 
		and sy.active = 1
	join CustomStudent cust on cust.personID = p.personID
		and cust.attributeID in (546,547,548,549)
		and cust.value is not null
	join v_CensusContactSummaryLite ccsl on ccsl.personID = p.personID
		and ccsl.guardian = 1
WHERE cal.calendarID IN (@calendarID)
	AND e.grade in ('07','08')
	and ccsl.addressLine1 is not null
group by p.studentNumber, i.lastName, i.firstName, e.grade, ccsl.addressLine1, ccsl.addressLine2
--order by i.lastName
) x
group by x.studentNumber, x.lastName, x.firstName, x.grade, x.addressLine1, x.addressLine2
order by x.grade, x.lastName, x.firstName</CommandText></Query><Fields><Field Name="studentNumber"><DataField>studentNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="lastName"><DataField>lastName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="firstName"><DataField>firstName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="grade"><DataField>grade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Semster_1"><DataField>Semster 1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Semster_2"><DataField>Semster 2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="addressLine1"><DataField>addressLine1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="addressLine2"><DataField>addressLine2</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="userCalendar"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@userID"><Value>=Parameters!userID.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @allCalendars bit
SELECT @allCalendars = u.allCalendars FROM dbo.UserAccount u WHERE u.userID = @userID

IF @allCalendars = 1
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as Calendar, 1 as ModifyRights, 1 as ownerRights
	FROM dbo.Calendar c
	INNER JOIN school s on s.schoolID = c.schoolID
	order by calendarID desc

END

ELSE
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as calendar , mr.modifyRights, mr.ownerRights
	FROM dbo.UserAccount ua
	LEFT OUTER JOIN dbo.UserSchoolYearRights mr on mr.userID = ua.userID
	INNER JOIN dbo.Calendar c on (mr.districtID = c.districtID)
					AND ((c.endYear = mr.endYear AND c.schoolID = mr.schoolID AND mr.calendarID IS NULL)
					OR (c.endYear = mr.endYear AND mr.schoolID IS NULL)
					OR (c.schoolID = mr.schoolID AND mr.endYear IS NULL)
					OR (c.calendarID = mr.calendarID)
					OR (mr.endYear IS NULL AND mr.schoolID IS NULL))
	INNER JOIN school s on s.schoolID = c.schoolID
	WHERE ua.userID = @userID
	UNION
	SELECT c2.districtID, c2.schoolID, c2.calendarID, c2.name + ' - ' + s2.name as calendar, mr2.modifyRights, mr2.ownerRights
	FROM dbo.UserGroupSchoolYearRights mr2
	INNER JOIN dbo.Calendar c2 on mr2.districtID = c2.districtID
					AND ((c2.endYear = mr2.endYear AND c2.schoolID = mr2.schoolID AND mr2.calendarID IS NULL)
					OR (c2.endYear = mr2.endYear AND mr2.schoolID IS NULL)
					OR (c2.schoolID = mr2.schoolID AND mr2.endYear IS NULL)
					OR (c2.calendarID = mr2.calendarID)
					OR (mr2.endYear IS NULL AND mr2.schoolID IS NULL))
	INNER JOIN UserGroupMember gm2 on gm2.groupID = mr2.groupID
	INNER JOIN school s2 on s2.schoolID = c2.schoolID
	WHERE gm2.userID = @userID
	ORDER BY calendarID DESC

END</CommandText></Query><Fields><Field Name="districtID"><DataField>districtID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="schoolID"><DataField>schoolID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="calendarID"><DataField>calendarID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Calendar"><DataField>Calendar</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ModifyRights"><DataField>ModifyRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ownerRights"><DataField>ownerRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix2"><TablixBody><TablixColumns><TablixColumn><Width>0.79167in</Width></TablixColumn><TablixColumn><Width>2.67708in</Width></TablixColumn><TablixColumn><Width>0.44792in</Width></TablixColumn><TablixColumn><Width>0.85417in</Width></TablixColumn><TablixColumn><Width>0.86458in</Width></TablixColumn><TablixColumn><Width>2.63542in</Width></TablixColumn><TablixColumn><Width>2.1875in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.25in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox5"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Student ID</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName>Textbox5</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox7"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Student Name</Va