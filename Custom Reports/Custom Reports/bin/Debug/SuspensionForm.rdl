<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>campus</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>3fd0d521-5636-4d8c-9650-c537adbec065</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="SuspensionForm"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@personID"><Value>=Parameters!personID.Value</Value></QueryParameter><QueryParameter Name="@incidentID"><Value>=Parameters!incidentID.Value</Value></QueryParameter></QueryParameters><CommandText>select p.studentNumber, i.firstName + ' ' +  i.lastName as [Student Name], i.gender, CONVERT(varchar(10), cast(i.birthdate as date),101) as birthdate, e.grade, cal.name as [School]
	, gc.firstName + ' ' + gc.lastName as [Parent/Guardian]
	, gc.Address + ', ' + gc.city + ', ' + gc.state + ' ' + gc.zip as [Address]
	, isnull(gc.householdPhone,isnull(gc.cellPhone,'')) as [Phone]
	, bi.incidentID
	, br.roleID
	, max(case when brt.code = '110' then 'On Campus'
			when brt.code = '100' then 'Off Campus' else '' end) as [SusType]
	, cd2.name as [Context]
	, cd.name as [Location]
	, CONVERT(varchar(10), cast(bi.timestamp as date),101) as [dateOfIncident]
	, max(case when bev.gn = 1 then bev.Event end) as [Event 1]
	, max(case when bev.gn = 2 then bev.Event end) as [Event 2]
	, max(case when bev.gn = 3 then bev.Event end) as [Event 3]
	, max(case when bev.gn = 4 then bev.Event end) as [Event 4]
	, max(case when bev.gn = 5 then bev.Event end) as [Event 5]
	, bres.schoolDaysDuration as [numDaysSuspended]
	, CONVERT(varchar(10), cast(bres.timestamp as date),101) + ' - ' + LTRIM(RIGHT(CONVERT(VARCHAR(20), bres.timestamp, 100), 7)) as [firstDayOfSuspension]
	, CONVERT(varchar(10), cast(bres.endTimeStamp as date),101) as [lastDayOfSuspension]
	--, max(case when sus.code = 'ISUS' then sus.count else '' end) as [Days OnCS]
	--, max(case when sus.code = 'SUS' then sus.count else '' end) as [Days OffCS]
	, bi.description as [Comments]
	, max(case when res.ParentContactType is not null then 'X' else '' end) as [ParentContact]
	, max(isnull(res.ParentContactType,'')) as [ContactType]
	, max(isnull(res.ParentContactDate,'')) as [ContactDate]
	, max(case when res.SJPDreportNum is not null then 'X' else '' end) as [Police Report]
	, max(isnull(CONVERT(varchar(10), cast(res.SJPDreportDate as date),101),'')) as [Report Date]
	, max(isnull(res.SJPDreportNum,'')) as [Case #]
	, MAX(CASE pp.programID WHEN '53' THEN 'X' else '' END) as [IEP]
	, MAX(CASE pp.programID WHEN '85' THEN 'X' else '' END) as [504 Plan]
	, MAX(CASE pp.programID WHEN '105' then 'X' else '' END) as [MKV]
	, max(case when lep.programStatus = 'LEP' then 'X' else '' END) as [LEP]
	, max(case when res.name = 'Expulsion Recommended' then 'X' else '' end) as [ExpulsionRecYes]
from Person p
	join [Identity] i on i.personID = p.personID
		and i.identityID = p.currentIdentityID
	join Enrollment e on e.personID = p.personID
	join Calendar cal on cal.calendarID = e.calendarID
		and cal.endYear = e.endYear
	join BehaviorRole br on br.personID = p.personID
		and br.calendarID = e.calendarID
	join BehaviorEvent be on be.eventID = br.eventID
		and be.calendarID = e.calendarID
	join BehaviorIncident bi on bi.incidentID = be.incidentID
		and bi.calendarID = e.calendarID
	join BehaviorType bt on bt.typeID = be.typeID
	join BehaviorResolution bres on br.roleID = bres.roleID
		and bres.calendarID = e.calendarID
	join BehaviorResType brt on brt.typeID = bres.typeID
		and brt.code in ('100','110')
	left join CampusDictionary cd on cd.code = bi.location
		and cd.attributeID = 726
	left join CampusDictionary cd2 on cd2.code = bi.context
		and cd2.attributeID = 724
	left join ProgramParticipation pp on pp.personID = p.personID
		and (pp.eligibilityEndDate is null or pp.eligibilityEndDate &gt;= bi.timestamp)
		and pp.programID in ('53','85','105')
	left join Lep on lep.personID = p.personID
	left join (
		select bi.incidentID, bi.title, bi.timestamp, be.eventID, br.personID
			, bt.code + ': ' +  bt.name + case when bd.drugID is not null then ' - Type: ' + cd.name 
												when bw.weaponID is not null then ' - Type: ' + cd2.name
												when bh.harassmentID is not null then ' - Type: ' + cd3.name
												else '' end as [Event]
			, ROW_NUMBER() over(PARTITION by bi.incidentID, br.personID order by be.mostSevere desc) as gn
		from BehaviorIncident bi
			join BehaviorEvent be on be.incidentID = bi.incidentID
			join BehaviorType bt on bt.typeID = be.typeID
			join BehaviorRole br on br.eventID = be.eventID
			left join BehaviorDrug bd on bd.roleID = br.roleID
			left join CampusDictionary cd on cd.code = bd.drugType
				and cd.attributeID = 1173
			left join BehaviorWeapon bw on bw.roleID = br.roleID
			left join CampusDictionary cd2 on cd2.code = bw.weaponType
				and cd2.attributeID = 1170
			left join BehaviorHarassment bh on bh.roleID = br.roleID
			left join CampusDictionary cd3 on cd3.code = bh.harassmentType
				and cd3.attributeID = 1098
			  ) bev on bev.incidentID = bi.incidentID and bev.personID = p.personID
	left join (
			select be.eventID, br.roleID, br.personID, brt.name
				, case when cbr.attributeID = 1518 then cd.name end as [ParentContactType]
				, case when cbr.attributeID = 1518 then CONVERT(varchar(10), cast(bres.timestamp as date),101) + ' - ' + LTRIM(RIGHT(CONVERT(VARCHAR(20), bres.timestamp, 100), 7)) end as [ParentContactDate]
				, case when cbr.attributeID = 1519 then cbr.value end as [SJPDreportNum]
				, case when cbr.attributeID = 1519 then bres.discAssignDate end as [SJPDreportDate]
			from BehaviorEvent be
				join BehaviorRole br on br.eventID = be.eventID
				join BehaviorResolution bres on bres.roleID = br.roleID
				join BehaviorResType brt on brt.typeID = bres.typeID
				left join CustomBehaviorResolution cbr on cbr.resolutionID = bres.resolutionID
					and cbr.attributeID in (1518,1519)
				left join CampusDictionary cd on cd.code = cbr.value
					and cd.attributeID = 1518
			where brt.name in ('SJ Police Contacted','Parent Contact','Expulsion Recommended','Law Enforcement Referal')
			  ) res on res.eventID = be.eventID and res.personID = p.personID and res.roleID = br.roleID
	left join (
		select stu.personID as stuID, rp.personID2 as [gdID], rp.seq, rp.name
			, ROW_NUMBER() over(PARTITION by stu.personID order by isnull(rp.seq, '100') asc) as gn 
			, gui.lastName, gui.firstname, gui.middleName, h.phone as householdPhone, co.cellPhone
			, co.homePhone as otherPhone, co.workPhone
			, case when a.postOfficeBox = 1 then 'PO Box ' + a.number else isnull(a.number + ISNULL(' ' + a.dir,'')  + ' ' + a.street + ' ' + isnull(a.tag,'') + ISNULL(' #' + a.apt, ''),'') end as [Address]
			, a.city, a.state, a.zip
		from Person stu 
			join RelatedPair rp on rp.personID1 = stu.personID
			left join Person gu on gu.personID = rp.personID2
			left join [Identity] gui on gui.personID = gu.personID and gui.identityID = gu.currentIdentityID
			left join HouseholdMember hmGu on hmGu.personID = rp.personID2 and hmGu.endDate is null
				and hmGu.personID = gu.personID
			left join Household h on h.householdID = hmGu.householdID
			left join HouseholdLocation hl on hl.householdID = h.householdID
				and hl.endDate is null
			left join Address a on a.addressID = hl.addressID
			left join Contact co on co.personID = rp.personID2
		where rp.guardian = 1
			  ) gc on gc.stuID = p.personID and gc.gn = 1
	--left join (
	--	select att.personID, att.code, COUNT(att.date) as [count]
	--	from(
	--		select p.personID, a.date, case when ae.code in ('SUS','SUS1') then 'SUS' else ae.code end as [code] 
	--		from Person p
	--			JOIN Attendance a ON a.personID = p.personID
	--			JOIN AttendanceExcuse ae ON ae.excuseID = a.excuseID
	--			join BehaviorRole br on br.personID = p.personID
	--			join BehaviorEvent be on be.eventID = br.eventID
	--			join BehaviorIncident bi on bi.incidentID = be.incidentID
	--				and bi.incidentID = @incidentID
	--			join BehaviorResolution bres on br.roleID = bres.roleID
	--		where ae.code in ('SUS','ISUS','SUS1')	 and a.date &lt;= bres.endTimeStamp and p.personID = @personID
	--		group by p.personID, a.date, ae.code
	--		) att
		--group by att.personID, att.code
		--	  ) sus on sus.personID = p.personID
where p.personID = @personID
	and bi.incidentID = @incidentID
group by p.studentNumber, i.firstName, i.lastName, i.gender, i.birthdate, e.grade, cal.name, bi.incidentID, brt.code, cd.name, cd2.name, bi.timestamp, bres.schoolDaysDuration
	, bres.timestamp, bres.endTimeStamp, bt.code, bi.description, gc.firstName, gc.lastName, gc.Address, gc.city, gc.state, gc.zip, gc.householdPhone, gc.cellPhone, br.roleID</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="studentNumber"><DataField>studentNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Student_Name"><DataField>Student Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="gender"><DataField>gender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="birthdate"><DataField>birthdate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="grade"><DataField>grade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="School"><DataField>School</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Parent_Guardian"><DataField>Parent/Guardian</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Address"><DataField>Address</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="incidentID"><DataField>incidentID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Phone"><DataField>Phone</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SusType"><DataField>SusType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Location"><DataField>Location</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="roleID"><DataField>roleID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="dateOfIncident"><DataField>dateOfIncident</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="numDaysSuspended"><DataField>numDaysSuspended</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="Event_1"><DataField>Event 1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Event_2"><DataField>Event 2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Context"><DataField>Context</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Event_3"><DataField>Event 3</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="firstDayOfSuspension"><DataField>firstDayOfSuspension</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Event_4"><DataField>Event 4</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="lastDayOfSuspension"><DataField>lastDayOfSuspension</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Event_5"><DataField>Event 5</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ParentContact"><DataField>ParentContact</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ContactType"><DataField>ContactType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ContactDate"><DataField>ContactDate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Police_Report"><DataField>Police Report</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Report_Date"><DataField>Report Date</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ExpulsionRecYes"><DataField>ExpulsionRecYes</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="IEP"><DataField>IEP</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ID504_Plan"><DataField>504 Plan</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="MKV"><DataField>MKV</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Comments"><DataField>Comments</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LEP"><DataField>LEP</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Case__"><DataField>Case #</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="IncidentStudents"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@incidentID"><Value>=Parameters!incidentID.Value</Value></QueryParameter></QueryParameters><CommandText>select p.personID, i.lastName + ', ' + i.firstName as student
from BehaviorIncident bi
	join BehaviorEvent be on be.incidentID = bi.incidentID
	join BehaviorRole br on br.eventID = be.eventID
	join Person p on p.personID = br.personID
	join [Identity] i on i.personID = p.personID
		and i.identityID = p.currentIdentityID
	join BehaviorResolution bres on bres.roleID = br.roleID
	join BehaviorResType brt on brt.typeID = bres.typeID
where brt.code in ('100','110')
	and bi.incidentID = @incidentID</CommandText></Query><Fields><Field Name="personID"><DataField>personID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="student"><DataField>student</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Rectangle Name="Rectangle1"><ReportItems><Textbox Name="Textbox19"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>OAK GROVE SCHOOL DISTRICT SUSPENSION FORM</Value><Style><FontFamily>Times New Roman</FontFamily><FontSize>12pt</FontSize><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName>Textbox19</rd:DefaultName><Top>0.15035in</Top><Left>1.69979in</Left><Height>0.30799in</Height><Width>4.48485in</Width><Style><Border><Style>None</Style></Border><VerticalAlign>Bottom</VerticalAlign><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><KeepTogether>true</KeepTogether><Top>0.05083in</Top><Left>0.24269in</Left><Height>0.45834in</Height><Width>8.02193in</Width><Style><Border><Style>None</Style></Border></Style></Rectangle><Textbox Name="Textbox22"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Birthdate:</Value><Style><FontFamily>Times New Roman</FontFamily><FontSize>9pt</FontSize></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox22</rd:DefaultName><Top>0.655in</Top><Left>3.45764in</Left><Height>0.25in</Height><Width>0.57292in</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border><VerticalAlign>Bottom</VerticalAlign><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Textbox Name="Textbox21">