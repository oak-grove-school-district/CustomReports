<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition"><DataSources><DataSource Name="campus"><DataSourceReference>campus</DataSourceReference><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">eb257eae-9596-43e0-8cf9-390831621d72</rd:DataSourceID><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType></DataSource></DataSources><DataSets><DataSet Name="AllStudents"><Fields><Field Name="Student_Number"><DataField>Student Number</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Calendar_Name"><DataField>Calendar Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Last_Name"><DataField>Last Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="First_Name"><DataField>First Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="grade"><DataField>Grade</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Incident__"><DataField>Incident #</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int64</rd:TypeName></Field><Field Name="HPL"><DataField>HPL</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Event_Date"><DataField>Event Date</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="Code_Contact_Mode"><DataField>Code/Contact Mode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Comments"><DataField>Comments</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>campus</DataSourceName><CommandText>select * from (
select ca.name as [Calendar Name], p.studentNumber as [Student Number], i.lastName as [Last Name], i.firstName as [First Name]
, e.grade as [Grade], i.homePrimaryLanguage as [HPL], ROW_NUMBER() over(PARTITION by p.studentNumber order by ma.date asc) as [Incident #]
, ma.date as [Event Date], ma.code as [Code/Contact Mode], ma.comments as [Comments]
from Person p
join [Identity] i on i.personID = p.personID and i.identityID = p.currentIdentityID
join enrollment e on e.personID = p.personID and (e.enddate &gt;= @date or e.enddate is null)
join calendar ca on ca.calendarID = e.calendarID and ca.calendarID in (@calendarID) and (ca.name like '%The%Academy%')
join School sc on sc.schoolID = ca.schoolID
join SchoolYear sy on sy.endYear = ca.endYear and sy.active = 1
join (
select x.personID, count(x.date) as [times] from (
select at.personID, at.date, aex.code, count(at.personID) as Marks
from attendance at
join AttendanceExcuse aex on aex.excuseID = at.excuseID and aex.code in (@attendanceCodes)
join SchoolYear sy on (at.date between sy.startDate and sy.endDate) and sy.active = 1
where at.date &gt;= sy.startDate and at.date &lt;= @date
group by at.personID, at.date, aex.code
having (count(at.personID) = 7 and aex.code not like 'TD30') or (count(at.personID) &lt; 7 and aex.code like 'TD30')
) x
group by x.personID
having COUNT(x.date) &gt;= @threshold
) ti on ti.personID = p.personID
join (
select at.personID, at.date, aex.code, at.comments, count(at.personID) as Marks
from attendance at
join AttendanceExcuse aex on aex.excuseID = at.excuseID and aex.code in (@attendanceCodes)
join SchoolYear sy on (at.date between sy.startDate and sy.endDate) and sy.active = 1
where at.date &gt;= sy.startDate and at.date &lt;= @date
group by at.personID, at.date, aex.code, at.comments
having (count(at.personID) = 7 and aex.code not like 'TD30') or (count(at.personID) &lt; 7 and aex.code like 'TD30')
) ma on ma.personID = p.personID
union
select ca.name, p.studentNumber, i.lastName, i.firstName, e.grade, i.homePrimaryLanguage, null, cl.dateTimeStamp, cl.contactMode, cl.text
from person p
join [Identity] i on i.personID = p.personID and i.identityID = p.currentIdentityID
join enrollment e on e.personID = p.personID and (e.enddate &gt;= @date or e.endDate is null)
join calendar ca on ca.calendarID = e.calendarID
join School sc on sc.schoolID = ca.schoolID
join SchoolYear sy on sy.endYear = ca.endYear and sy.active = 1
join (
select x.personID, count(x.date) as [times] from (
select at.personID, at.date, aex.code, count(at.personID) as Marks
from attendance at
join AttendanceExcuse aex on aex.excuseID = at.excuseID and aex.code in (@attendanceCodes)
join SchoolYear sy on (at.date between sy.startDate and sy.endDate) and sy.active = 1
where at.date &gt;= sy.startDate and at.date &lt;= @date
group by at.personID, at.date, aex.code
having (count(at.personID) = 7 and aex.code not like 'TD30') or (count(at.personID) &lt; 7 and aex.code like 'TD30')
) x
group by x.personID
having COUNT(x.date) &gt;= @threshold
) ti on ti.personID = p.personID
join (
select at.personID, at.date, aex.code, at.comments, count(at.personID) as Marks
from attendance at
join AttendanceExcuse aex on aex.excuseID = at.excuseID and aex.code in (@attendanceCodes)
join SchoolYear sy on (at.date between sy.startDate and sy.endDate) and sy.active = 1
where at.date &gt;= sy.startDate and at.date &lt;= @date
group by at.personID, at.date, aex.code, at.comments
having (count(at.personID) = 7 and aex.code not like 'TD30') or (count(at.personID) &lt; 7 and aex.code like 'TD30')
) ma on ma.personID = p.personID
join ContactLog cl on cl.personID = p.personID and cl.module = 'PLP' and cl.dateTimeStamp &gt;= ca.startDate
) y
order by y.[Calendar Name], y.[Last Name], y.[First Name], y.[Event Date]</CommandText><QueryParameters><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter><QueryParameter Name="@date"><Value>=Parameters!date.Value</Value></QueryParameter><QueryParameter Name="@attendanceCodes"><Value>=split(Parameters!attendanceCodes.Value,",")</Value></QueryParameter><QueryParameter Name="@threshold"><Value>=Parameters!threshold.Value</Value></QueryParameter></QueryParameters><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="userCalendar"><Fields><Field Name="districtID"><DataField>districtID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="schoolID"><DataField>schoolID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="calendarID"><DataField>calendarID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Calendar"><DataField>Calendar</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="ModifyRights"><DataField>ModifyRights</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="ownerRights"><DataField>ownerRights</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields><Query><DataSourceName>campus</DataSourceName><CommandText>DECLARE @allCalendars bit
SELECT @allCalendars = u.allCalendars FROM dbo.UserAccount u WHERE u.userID = @userID

IF @allCalendars = 1
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as Calendar, 1 as ModifyRights, 1 as ownerRights
	FROM dbo.Calendar c
	INNER JOIN school s on s.schoolID = c.schoolID
	order by calendarID desc

END

ELSE
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as calendar , mr.modifyRights, mr.ownerRights
	FROM dbo.UserAccount ua
	LEFT OUTER JOIN dbo.UserSchoolYearRights mr on mr.userID = ua.userID
	INNER JOIN dbo.Calendar c on (mr.districtID = c.districtID)
					AND ((c.endYear = mr.endYear AND c.schoolID = mr.schoolID AND mr.calendarID IS NULL)
					OR (c.endYear = mr.endYear AND mr.schoolID IS NULL)
					OR (c.schoolID = mr.schoolID AND mr.endYear IS NULL)
					OR (c.calendarID = mr.calendarID)
					OR (mr.endYear IS NULL AND mr.schoolID IS NULL))
	INNER JOIN school s on s.schoolID = c.schoolID
	WHERE ua.userID = @userID
	UNION
	SELECT c2.districtID, c2.schoolID, c2.calendarID, c2.name + ' - ' + s2.name as calendar, mr2.modifyRights, mr2.ownerRights
	FROM dbo.UserGroupSchoolYearRights mr2
	INNER JOIN dbo.Calendar c2 on mr2.districtID = c2.districtID
					AND ((c2.endYear = mr2.endYear AND c2.schoolID = mr2.schoolID AND mr2.calendarID IS NULL)
					OR (c2.endYear = mr2.endYear AND mr2.schoolID IS NULL)
					OR (c2.schoolID = mr2.schoolID AND mr2.endYear IS NULL)
					OR (c2.calendarID = mr2.calendarID)
					OR (mr2.endYear IS NULL AND mr2.schoolID IS NULL))
	INNER JOIN UserGroupMember gm2 on gm2.groupID = mr2.groupID
	INNER JOIN school s2 on s2.schoolID = c2.schoolID
	WHERE gm2.userID = @userID
	ORDER BY calendarID DESC

END</CommandText><QueryParameters><QueryParameter Name="@userID"><Value>=Parameters!userID.Value</Value></QueryParameter></QueryParameters><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="grade"><Fields><Field Name="grade"><DataField>grade</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="seq"><DataField>seq</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Byte</rd:TypeName></Field></Fields><Query><DataSourceName>campus</DataSourceName><CommandText>SELECT distinct e.grade, gl.seq
FROM enrollment e
INNER JOIN gradelevel gl on gl.[name] = e.grade AND gl.calendarID = e.calendarID
WHERE e.calendarID in (@calendarID)
ORDER BY gl.seq</CommandText><QueryParameters><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter></QueryParameters><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="currentCalendars"><Fields><Field Name="currentCalendarID"><DataField>currentCalendarID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields><Query><DataSourceName>campus</DataSourceName><CommandText>DECLARE @allCalendars bit
SELECT @allCalendars = u.allCalendars FROM dbo.UserAccount u WHERE u.userID = @userID

IF @allCalendars = 1
BEGIN

	select * from (
	SELECT case when sy.active = 1 then c.calendarID end as currentCalendarID 
	FROM dbo.Calendar c
	INNER JOIN schoolYear sy on sy.endYear = c.endYear
	INNER JOIN school s on s.schoolID = c.schoolID
	) x
	where x.[currentCalendarID] is not null

END

ELSE
BEGIN

	select * from (
	SELECT case when sy.active = 1 then c.calendarID end as currentCalendarID
	FROM dbo.UserAccount ua
	LEFT OUTER JOIN dbo.UserSchoolYearRights mr on mr.userID = ua.userID
	INNER JOIN dbo.Calendar c on (mr.districtID = c.districtID)
					AND ((c.endYear = mr.endYear AND c.schoolID = mr.schoolID AND mr.calendarID IS NULL)
					OR (c.endYear = mr.endYear AND mr.schoolID IS NULL)
					OR (c.schoolID = mr.schoolID AND mr.endYear IS NULL)
					OR (c.calendarID = mr.calendarID)
					OR (mr.endYear IS NULL AND mr.schoolID IS NULL))
	INNER JOIN school s on s.schoolID = c.schoolID
	INNER JOIN schoolYear sy on sy.endYear = c.endYear
	WHERE ua.userID = @userID
	) x
	where x.[currentCalendarID] is not null
	UNION
	select * from (
	SELECT case when sy.active = 1 then c2.calendarID end as currentCalendarID
	FROM dbo.UserGroupSchoolYearRights mr2
	INNER JOIN dbo.Calendar c2 on mr2.districtID = c2.districtID
					AND ((c2.endYear = mr2.endYear AND c2.schoolID = mr2.schoolID AND mr2.calendarID IS NULL)
					OR (c2.endYear = mr2.endYear AND mr2.schoolID IS NULL)
					OR (c2.schoolID = mr2.schoolID AND mr2.endYear IS NULL)
					OR (c2.calendarID = mr2.calendarID)
					OR (mr2.endYear IS NULL AND mr2.schoolID IS NULL))
	INNER JOIN UserGroupMember gm2 on gm2.groupID = mr2.groupID
	INNER JOIN school s2 on s2.schoolID = c2.schoolID
	INNER JOIN schoolYear sy on sy.endYear = c2.endYear
	WHERE gm2.userID = @userID
	) y
	where y.[currentCalendarID] is not null

END</CommandText><QueryParameters><QueryParameter Name="@userID"><Value>=Parameters!userID.Value</Value></QueryParameter></QueryParameters><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="settings"><Fields><Field Name="code"><DataField>code</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>campus</DataSourceName><CommandText>select case
when @letterName like 'Excessive Illness%' then 'ILL' 
else 'OTX,UNV,TD30,CUT' end as [code]</CommandText><QueryParameters><QueryParameter Name="@letterName"><Value>=Parameters!letterName.Value</Value></QueryParameter></QueryParameters><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="thresholds"><Fields><Field Name="threshold"><DataField>threshold</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field></Fields><Query><DataSourceName>campus</DataSourceName><CommandText>select case 
when @letterName like 'Form A%' then 3.0
when @letterName like 'Form B%' then 5.0
when @letterName like 'Excessive Illness%' then 7.0
end as [threshold]</CommandText><QueryParameters><QueryParameter Name="@letterName"><Value>=Parameters!letterName.Value</Value></QueryParameter></QueryParameters><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet></DataSets><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>0.33333in</Width></TablixColumn><TablixColumn><Width>0.92708in</Width></TablixColumn><TablixColumn><Width>0.79167in</Width></TablixColumn><TablixColumn><Width>1.76042in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.15625in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox4"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Incident </Value><Style><FontSize>6pt</FontSize></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox4</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox7"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Event Date</Value><Style><FontSize>6pt</FontSize></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox7</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox9"><CanGrow>true</CanGrow><KeepTo