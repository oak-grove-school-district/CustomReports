<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>campus</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>2ccc00c1-4ca9-4375-a6fa-9c12991dc04b</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="userCalendar"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@userID"><Value>=Parameters!userID.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @allCalendars bit
SELECT @allCalendars = u.allCalendars FROM dbo.UserAccount u WHERE u.userID = @userID

IF @allCalendars = 1
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as Calendar, 1 as ModifyRights, 1 as ownerRights
	FROM dbo.Calendar c
	INNER JOIN school s on s.schoolID = c.schoolID
	order by calendarID desc

END

ELSE
BEGIN

	SELECT c.districtID, c.schoolID, c.calendarID, c.name + ' - ' + s.name as calendar , mr.modifyRights, mr.ownerRights
	FROM dbo.UserAccount ua
	LEFT OUTER JOIN dbo.UserSchoolYearRights mr on mr.userID = ua.userID
	INNER JOIN dbo.Calendar c on (mr.districtID = c.districtID)
					AND ((c.endYear = mr.endYear AND c.schoolID = mr.schoolID AND mr.calendarID IS NULL)
					OR (c.endYear = mr.endYear AND mr.schoolID IS NULL)
					OR (c.schoolID = mr.schoolID AND mr.endYear IS NULL)
					OR (c.calendarID = mr.calendarID)
					OR (mr.endYear IS NULL AND mr.schoolID IS NULL))
	INNER JOIN school s on s.schoolID = c.schoolID
	WHERE ua.userID = @userID
	UNION
	SELECT c2.districtID, c2.schoolID, c2.calendarID, c2.name + ' - ' + s2.name as calendar, mr2.modifyRights, mr2.ownerRights
	FROM dbo.UserGroupSchoolYearRights mr2
	INNER JOIN dbo.Calendar c2 on mr2.districtID = c2.districtID
					AND ((c2.endYear = mr2.endYear AND c2.schoolID = mr2.schoolID AND mr2.calendarID IS NULL)
					OR (c2.endYear = mr2.endYear AND mr2.schoolID IS NULL)
					OR (c2.schoolID = mr2.schoolID AND mr2.endYear IS NULL)
					OR (c2.calendarID = mr2.calendarID)
					OR (mr2.endYear IS NULL AND mr2.schoolID IS NULL))
	INNER JOIN UserGroupMember gm2 on gm2.groupID = mr2.groupID
	INNER JOIN school s2 on s2.schoolID = c2.schoolID
	WHERE gm2.userID = @userID
	ORDER BY calendarID DESC

END</CommandText></Query><Fields><Field Name="districtID"><DataField>districtID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="schoolID"><DataField>schoolID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="calendarID"><DataField>calendarID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Calendar"><DataField>Calendar</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ModifyRights"><DataField>ModifyRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ownerRights"><DataField>ownerRights</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="Languages"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@calendarID"><Value>=Parameters!calendarID.Value</Value></QueryParameter><QueryParameter Name="@language"><Value>=Parameters!language.Value</Value></QueryParameter></QueryParameters><CommandText>select p.studentNumber, i.lastName, i.firstName, cd.name as [Home Primary Language]
from Person p
	join [Identity] i on i.personID = p.personID
		and i.identityID = p.currentIdentityID
	join Enrollment e on e.personID = p.personID
		and e.active = 1
		and (e.endDate is null or e.endDate &gt;= getdate())
	join Calendar cal on cal.calendarID = e.calendarID
		and cal.endYear = e.endYear
		and cal.calendarID = @calendarID
	join CampusDictionary cd on cd.code = i.homePrimaryLanguage
		and cd.attributeID = 280
where cd.code = @language</CommandText></Query><Fields><Field Name="studentNumber"><DataField>studentNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="lastName"><DataField>lastName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="firstName"><DataField>firstName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Home_Primary_Language"><DataField>Home Primary Language</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="langselect"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>SELECT        code, CAST(code AS varchar) + ' - ' + name AS name
FROM            CampusDictionary
WHERE        (attributeID = 280)</CommandText></Query><Fields><Field Name="code"><DataField>code</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="name"><DataField>name</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>1in</Width></TablixColumn><TablixColumn><Width>1.78125in</Width></TablixColumn><TablixColumn><Width>1.75in</Width></TablixColumn><TablixColumn><Width>2.4375in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.25in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox1"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Student ID</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox1</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox3"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Last Name</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox3</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox5"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>First Name</Value><Style><FontFamily>Segoe UI</FontFamily></Style></TextRun>