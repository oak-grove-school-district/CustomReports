<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>campus</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>7736db1f-e469-48cd-8af7-30c4021fc843</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Language"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@endyear"><Value>=Parameters!endyear.Value</Value></QueryParameter><QueryParameter Name="@calendar"><Value>=Parameters!calendar.Value</Value></QueryParameter><QueryParameter Name="@startdate"><Value>=Parameters!startdate.Value</Value></QueryParameter></QueryParameters><CommandText>select lang.label as [School Year], lang.[School Name], lang.stateID as SSID, lang.lastName + ', ' + lang.firstName as studentName, lang.programStatus as [LEP Status],
	CONVERT(varchar(10), cast(lang.identifiedDate as date),101) as identifiedDate
	, CONVERT(varchar(10), cast(lang.expectedExitDate as date),101) as expectedExitDate
	, CONVERT(varchar(10), cast(lang.exitDate as date),101) as exitDate
	, lang.homePrimaryLang,
/* TO PIVOT THE ROW VALUES TO COLUMN NAMES AND DISPLAY CORRESPONDING LANGUAGE NAME */
/* MIN/MAX IS A TRICK TO NOT AGGREGATE DATA */
	MIN(case lang.attributeID when '280' then lang.language end) as homeLanguage,
	MIN(case lang.attributeID when '341' then lang.language end) as correspondenceLanguage,
	MIN(case lang.attributeID when '342' then lang.language end) as studentSpokenTo,
	MIN(case lang.attributeID when '343' then lang.language end) as adultSpokenLanguage
from 
/* SUB QUERY FOR TABLE YOU WANT TO PIVOT */
(		select cs.attributeID, s.lastName, s.firstName, cd.name as [language],
			cd2.name as homePrimaryLang, s.stateID, c.name as [School Name], sy.label, lep.programStatus,
			lep.identifiedDate, lep.expectedExitDate, Lep.exitDate

		from student s
		join Calendar c on s.calendarID = c.calendarID	/* UNCOMMENT IF FILTERING BY SCHOOL YEAR */
		join SchoolYear sy on sy.endYear = @endyear
		left join CustomStudent cs on cs.personID = s.personID
			and (cs.attributeID = 341 or cs.attributeID = 342 or cs.attributeID = 343 or cs.attributeID = 280)
		left join CampusDictionary cd on cd.attributeID = cs.attributeID
			and cd.code = cs.value
		left join CampusDictionary cd2 on cd2.attributeID = 341
			and cd2.code = s.homePrimaryLanguage
		left join Lep on lep.personID = s.personID
		
		where c.calendarID in (@calendar)
			and s.startDate &gt;= @startdate

) as lang
group by lang.label, lang.stateID, lang.lastName, lang.firstName, lang.homePrimaryLang, lang.[School Name], lang.programStatus, lang.identifiedDate, lang.expectedExitDate, lang.exitDate		/* NON-PIVOTED COLUMNS */
order by lang.lastName, lang.firstName</CommandText></Query><Fields><Field Name="School_Year"><DataField>School Year</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="School_Name"><DataField>School Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SSID"><DataField>SSID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="studentName"><DataField>studentName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LEP_Status"><DataField>LEP Status</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="identifiedDate"><DataField>identifiedDate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="expectedExitDate"><DataField>expectedExitDate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="exitDate"><DataField>exitDate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="homePrimaryLang"><DataField>homePrimaryLang</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="homeLanguage"><DataField>homeLanguage</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="correspondenceLanguage"><DataField>correspondenceLanguage</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="studentSpokenTo"><DataField>studentSpokenTo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="adultSpokenLanguage"><DataField>adultSpokenLanguage</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="SchoolYear"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>select distinct sy.label, sy.endYear
from SchoolYear sy
order by sy.endYear desc</CommandText></Query><Fields><Field Name="label"><DataField>label</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="endYear"><DataField>endYear</DataField><rd:TypeName>System.Int16</rd:TypeName></Field></Fields></DataSet><DataSet Name="StartDate"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@endyear"><Value>=Parameters!endyear.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        calendarID, name, startDate
