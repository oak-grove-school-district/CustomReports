    <DataSet Name="AllStudents">
      <Fields>
        <Field Name="lastName">
          <DataField>lastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="firstName">
          <DataField>firstName</DataField>
