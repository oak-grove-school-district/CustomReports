        </Field>
        <Field Name="grade">
          <DataField>grade</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Guardian1Email">
          <DataField>Guardian1Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Guardian2Name">
          <DataField>Guardian2Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="studentnumber">
          <DataField>studentNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Guardian2Username">
          <DataField>Guardian2Username</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastAccessed">
          <DataField>lastAccessed</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Guardian2Email">
          <DataField>Guardian2Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>campus</DataSourceName>
        <CommandText>with Guardians (stuID, gdID, gn, householdID, lastName, firstName, email) as (
select stu.personID as stuID, rp.personID2 as [gdID]
, ROW_NUMBER() over(PARTITION by stu.personID order by isnull(rp.seq, '100') asc) as gn 
, h.householdID
, gui.lastName
, gui.firstname
, case when c.email like '%@oakgrovesd.net%' then c.secondaryEmail else c.email end as [email]
from Person stu 
join RelatedPair rp on rp.personID1 = stu.personID and rp.guardian = 1
join HouseholdMember hmStu on hmStu.personID = rp.personID1 and hmStu.endDate is null
join HouseholdMember hmGu on hmGu.personID = rp.personID2 and rp.guardian = 1
join Household h on h.householdID = hmStu.householdID and h.householdID = hmGu.householdID
join Person gu on gu.personID = rp.personID2
join [Identity] gui on gui.personID = gu.personID and gui.identityID = gu.currentIdentityID
left join Contact c on c.personID = gu.personID
)

select p.studentNumber, i.lastName, i.firstName, e.grade
, isnull(MAX(case when gs.gn = 1 then gs.lastName + ', ' + gs.firstName end),'') as [Guardian1Name]
, isnull(MAX(case when gs.gn = 1 then gs.email end),'') as [Guardian1Email]
, isnull(MAX(case when gs.gn = 1 then ua.username end),'') as Guardian1Username
, isnull(MAX(case when gs.gn = 2 then gs.lastName + ', ' + gs.firstName end),'') as [Guardian2Name]
, isnull(MAX(case when gs.gn = 2 then gs.email end),'') as [Guardian2Email]
, isnull(MAX(case when gs.gn = 2 then ua.username end),'') as Guardian2Username
, isnull(convert(varchar(30),max(sa.timestamp),101),'') as [lastAccessed]

from Person p
join [Identity] i on i.personID = p.personID and i.identityID = p.currentIdentityID
left join IdentityRaceEthnicity ire on ire.identityID = i.identityID
join Enrollment e on e.personID = p.personID and e.calendarID in (@calendarID) and e.grade in (@grade) and e.endDate is null
join SchoolYear sy on sy.endYear = e.endYear and sy.active = 1
left join Guardians gs on gs.stuID = p.personID
left join UserAccount ua on ua.personID = gs.gdID
left join SecurityAccess sa on sa.username = ua.username and sa.timestamp &gt;= '08/17/2015'
group by p.studentNumber, i.lastName, i.firstName, e.grade
order by i.lastName, i.firstName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@grade">
            <Value>=Parameters!grade.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
