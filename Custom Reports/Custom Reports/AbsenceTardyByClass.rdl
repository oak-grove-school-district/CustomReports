    <DataSet Name="attendance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sectionID">
            <Value>=Parameters!sectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select x.studentNumber, x.lastName, x.firstName, x.grade, x.name, x.teacherDisplay
	,sum(case when x.status = 'A' and x.excuse = 'E' then 1 else 0 end) as [ExcusedAbsences]
	,sum(case when x.status = 'A' and x.excuse = 'U' then 1 else 0 end) as [UnexcusedAbsences]
	,sum(case when x.[Att Code] = 'T' or x.status = 'T' then 1 else 0 end) as [Tardies]
from
(
		SELECT p.studentNumber, i.lastName, i.firstName, e.grade, c.name, a.date
			, case when a.status = 'T' and a.excuseID is null then 'T'
				when a.status = 'A' and a.excuseID is null then 'A' else ae.code end as [Att Code]
			, ae.status
			, ae.excuse
			, sec.teacherDisplay
		from Person p
			join [Identity] i on i.personID = p.personID
				and i.identityID = p.currentIdentityID
			join Enrollment e on e.personID = p.personID
				and e.startDate &lt;= getdate()
				and (e.endDate &lt;= getdate() or e.endDate is null)
			join Calendar c on c.calendarID = e.calendarID
				and c.endYear = case when getdate() between c.startDate and c.endDate then c.endYear end
				AND (e.startDate &lt;= c.endDate
				AND (e.endDate &gt;= c.startDate OR e.endDate IS NULL))
			INNER JOIN ScheduleStructure AS ss ON ss.calendarID = c.calendarID
			INNER JOIN Trial AS tr ON tr.structureID = ss.structureID
				AND tr.active = 1
			INNER JOIN Course cs on cs.calendarID = c.calendarID
				and cs.active = 1
			INNER JOIN Section AS sec ON sec.courseID = cs.courseID
				and sec.trialID = tr.trialID
			INNER JOIN Roster r on r.sectionID = sec.sectionID
				and r.trialID = tr.trialID
				and (r.endDate is null or r.endDate &gt; getdate())
				and r.personID = p.personID
			JOIN Attendance a ON a.personID = p.personID
				AND a.date BETWEEN c.startDate AND c.endDate
				and a.date &lt;= getdate()
				and a.calendarID = c.calendarID
			LEFT JOIN AttendanceExcuse ae ON ae.excuseID = a.excuseID
				and ae.calendarID = c.calendarID
		where c.calendarID in (@calendarID)
			and sec.sectionID in (@sectionID)
			and ae.code not in ('ECO')
			and ae.status not like 'P'
		group by p.studentNumber, i.lastName, i.firstName, e.grade, c.name, a.date, ae.code, a.status, a.excuseID, ae.status, ae.excuse, sec.teacherDisplay
) x
group by x.studentNumber, x.lastName, x.firstName, x.grade, x.name, x.teacherDisplay
order by x.teacherDisplay, x.lastName</CommandText>
      </Query>
      <Fields>
        <Field Name="studentNumber">
          <DataField>studentNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lastName">
          <DataField>lastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="firstName">
          <DataField>firstName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
