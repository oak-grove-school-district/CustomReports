    <DataSet Name="section">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@personID">
            <Value>=Parameters!personID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select sec.sectionID, c.name + ' - ' + convert(varchar,sec.number) as Section
from Section sec
	join Course c on c.courseID = sec.courseID
	join Calendar cal on cal.calendarID = c.calendarID
		and cal.calendarID = @calendarID
	join Trial tr on tr.calendarID = cal.calendarID
		and tr.active = 1
		and tr.trialID = sec.trialID
where sec.teacherPersonID = @personID
group by sec.sectionID, c.name, sec.number</CommandText>
      </Query>
      <Fields>
        <Field Name="sectionID">
          <DataField>sectionID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Section">
          <DataField>Section</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
