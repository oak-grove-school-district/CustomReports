        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="teacherDisplay">
          <DataField>teacherDisplay</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ExcusedAbsences">
          <DataField>ExcusedAbsences</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UnexcusedAbsences">
          <DataField>UnexcusedAbsences</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tardies">
          <DataField>Tardies</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
