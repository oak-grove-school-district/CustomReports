    <DataSet Name="grades">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select gl.name
from GradeLevel gl
where gl.calendarID in (@calendarID)
and gl.name not in ('PK','SO','08')
group by gl.name
order by gl.name</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="APC">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@endYear">
            <Value>=Parameters!endYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@grade">
            <Value>=Parameters!grade.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select x.code, case when x.code like '' then '' else x.code + ': ' + x.name end as [Title]
from(
	select isnull(max(case when cs.attributeID = 470 then cs.value end),'') as code
		, isnull(max(case when cs.attributeID = 470 then cd.name end),'') as name
	from Person p
		join [Identity] i on i.personID = p.personID
			and i.identityID = p.currentIdentityID
		join Enrollment e on e.personID = p.personID
			and e.startDate &lt;= getdate()
			and (e.endDate is null or e.endDate &gt;= getdate())
		join Calendar cal on cal.calendarID = e.calendarID
			and cal.endYear = e.endYear
			and cal.endYear = @endYear
		left join CustomStudent cs on cs.personID = p.personID
			and cs.enrollmentID = e.enrollmentID
			and cs.attributeID in (470)
		left join CampusDictionary cd on cd.code = cs.value
			and cd.attributeID = cs.attributeID
	where e.grade in (@grade) and cal.calendarID in (@calendarID)
	group by cs.value, cs.attributeID
	) x
order by x.code, x.name</CommandText>
      </Query>
      <Fields>
        <Field Name="code">
          <DataField>code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AFC">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@endYear">
            <Value>=Parameters!endYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@grade">
            <Value>=Parameters!grade.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select x.code, case when x.code like '' then '' else x.code + ': ' + x.name end as [Title]
from(
	select isnull(max(case when cs.attributeID = 123 then cs.value end),'') as code
		, isnull(max(case when cs.attributeID = 123 then cd.name end),'') as name
	from Person p
		join [Identity] i on i.personID = p.personID
			and i.identityID = p.currentIdentityID
		join Enrollment e on e.personID = p.personID
			and e.startDate &lt;= getdate()
			and (e.endDate is null or e.endDate &gt;= getdate())
		join Calendar cal on cal.calendarID = e.calendarID
			and cal.endYear = e.endYear
			and cal.endYear = @endYear
		left join CustomStudent cs on cs.personID = p.personID
			and cs.enrollmentID = e.enrollmentID
			and cs.attributeID in (123)
		left join CampusDictionary cd on cd.code = cs.value
			and cd.attributeID = cs.attributeID
	where e.grade in (@grade) and cal.calendarID in (@calendarID)
	group by cs.value, cs.attributeID
	) x
order by x.code, x.name</CommandText>
      </Query>
      <Fields>
        <Field Name="code">
          <DataField>code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="nextCalendar">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@endYear">
            <Value>=Parameters!endYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@calendarID">
            <Value>=Parameters!calendarID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        CASE WHEN nSchool LIKE '' THEN 'No Enrollment' ELSE nSchool END AS nSchool, ncalID
FROM            (SELECT        p.studentNumber, i.lastName, i.firstName, e.grade AS cGrade, cal.name AS cSchool, cal.calendarID, e.endDate, ISNULL(MAX(CASE WHEN cs.attributeID = 123 THEN cs.value END), '') AS AFC, 
                                                    ISNULL(MAX(CASE WHEN cs.attributeID = 470 THEN cs.value END), '') AS APC, ISNULL(e.nextGrade, '') AS nGrade, ISNULL(cal2.name, '') AS nSchool, isnull(cal2.calendarID,'') AS nCalID, 
                                                    isnull(CASE WHEN e.grade IN ('TK', '00', '01', '02', '03', '04', '05') THEN sb.[New Home Elementary] WHEN e.grade IN ('06', '07') THEN sb.[New Home Jr. High] END, '') AS [Home School], isnull(sb.District, '') 
                                                    AS [District]
                          FROM            Person AS p INNER JOIN
                                                    [Identity] AS i ON i.personID = p.personID AND i.identityID = p.currentIdentityID INNER JOIN
                                                    Enrollment AS e ON e.personID = p.personID AND e.startDate &lt;= GETDATE() /*AND (e.endDate IS NULL OR e.endDate &gt;= GETDATE()) */ INNER JOIN
                                                    Calendar AS cal ON cal.calendarID = e.calendarID AND cal.endYear = e.endYear AND cal.endYear = @endYear LEFT OUTER JOIN
                                                    CustomStudent AS cs ON cs.personID = p.personID AND cs.enrollmentID = e.enrollmentID AND cs.attributeID IN (123, 470) LEFT OUTER JOIN
                                                    Calendar AS cal2 ON cal2.calendarID = e.nextCalendar LEFT JOIN
                                                    HouseholdMember hm ON hm.personID = p.personID AND (hm.secondary = 0 OR
                                                    hm.secondary IS NULL) AND (hm.endDate IS NULL OR
                                                    hm.endDate &gt; getdate()) LEFT JOIN
                                                    Household hh ON hh.householdID = hm.householdID LEFT JOIN
                                                    HouseholdLocation hl ON hl.householdID = hh.householdID AND (hl.endDate IS NULL OR
                                                    hl.endDate &gt; GETDATE()) AND (hl.secondary = 0 OR
                                                    hl.secondary IS NULL) LEFT JOIN
                                                        (SELECT        ad.addressID, nb.[Home Elementary] AS [New Home Elementary], nb.[Home Jr. High] AS [New Home Jr. High], dis.name AS [District]
                                                          FROM            [Address] ad LEFT JOIN
                                                                                        (SELECT        hs.addressID, COALESCE (max(CASE WHEN hs.bn = 1 AND hs.schID &lt;= 16 THEN sc.name END), '') + COALESCE (' or ' + max(CASE WHEN hs.bn = 2 AND 
                                                                                                                    hs.schID &lt;= 16 THEN sc.name END), '') AS [Home Elementary], COALESCE (max(CASE WHEN ((hs.bn = 2 AND hs.schID &gt;= 17) OR
                                                                                                                    (hs.bn = 3 AND hs.schID &gt;= 17)) THEN sc.name END), '') + COALESCE (' or ' + max(CASE WHEN hs.bn = 4 AND hs.schID &gt;= 17 THEN sc.name ELSE NULL END), '') 
                                                                                                                    AS [Home Jr. High]
                                                                                          FROM            (SELECT        hl.addressID, sb.schoolID AS schID, ROW_NUMBER() OVER (PARTITION BY hl.addressID
                                                                                                                    ORDER BY sb.schoolID) AS bn
                                                                                          FROM            HouseholdLocation hl JOIN
                                                                                                                    SchoolBoundary sb ON sb.addressID = hl.addressID
                                                                                          WHERE        sb.startDate = '03/08/2018'
                                                                                          GROUP BY hl.addressID, sb.schoolID) hs JOIN
                                                                                    School sc ON sc.schoolID = hs.schID
                                                          GROUP BY hs.addressID) nb ON nb.addressID = ad.addressID LEFT JOIN
                                                    District dis ON dis.districtID = ad.districtID) sb ON sb.addressID = hl.addressID AND (hl.secondary = 0 OR
                         hl.secondary IS NULL) AND (hm.secondary = 0 OR
                         hm.secondary IS NULL)
GROUP BY p.studentNumber, i.lastName, i.firstName, cal.name, cal2.name, e.endDate, e.grade, e.nextGrade, e.nextCalendar, cal.calendarID, sb.[New Home Elementary], sb.[New Home Jr. High], sb.District, cal2.calendarID) AS x
WHERE        (calendarID IN (@calendarID))
GROUP BY nSchool, nCalID
ORDER BY nSchool, nCalID</CommandText>
      </Query>
      <Fields>
        <Field Name="nSchool">
          <DataField>nSchool</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="nCalID">
          <DataField>ncalID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
